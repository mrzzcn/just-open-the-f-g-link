{
  "manifest_version": 2,

  "name": "Just open the f***g link",
  "description": "Just open the f***g link.",
  "version": "1.0",
  "icons": { "128": "icon.png" },
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "permissions": [
    "webNavigation",
    "declarativeContent",
    "activeTab",
    "storage",
    "tabs",
    "*://*/*"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  }
}
